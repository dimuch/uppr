    "clsx": "^2.1.1",
    "eslint-config-next": "^16.0.5",
    "mysql2": "^3.11.5",
    "next": "^16.0.5",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
